{
  "metadata": [
    {
      "src": [
        {
          "src": "../.",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "./Compiler"
    }
  ],
  "build": {
    "content": [
      {
        "src": ".",
        "files": "Compiler/**.yml"
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "./_site",
    "template": [
      "statictoc"
    ],
    "globalMetadata": {
      "_appName": "OSCLAN",
      "_appTitle": "OSCLAN",
      "_enableSearch": true,
      "pdf": true
    }
  }
}